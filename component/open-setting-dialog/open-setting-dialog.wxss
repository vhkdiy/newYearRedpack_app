.open_setting_dialog{
  width : 100%;
  height : 100%;
  position: fixed;
  top : 0;
  left : 0;
  background-color : rgba(0,0,0,0.5);
  z-index : 99999;
}
.open_setting_container{
  position: absolute;
  top : 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width : 648rpx;
  height: 407rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
}
.open_setting_container .title{
  margin-top : 48rpx;
  text-align: center;
  font-family: PingFangSC-Semibold;
  font-size: 42rpx;
  color: #333333;
  letter-spacing: 0;
}

.open_setting_container .subtitle{
  padding: 0 0;
  margin : 36rpx 51rpx 48rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #666666;
  letter-spacing: 0;
  line-height: 1.5;
  word-wrap:break-word !important; 
  word-break:break-all !important;
  white-space: normal;
}
.open_setting_container .img{
  position: absolute;
  top : 0;
  right : 0;
  width : 90rpx;
  height : 90rpx;
}
.open_setting_container .cancel{
  display: inline-block;
  overflow: inherit;
  padding: 0;
  background-color: transparent;
  margin : 0 30rpx;

  width : 264rpx;
  height : 78rpx;
  line-height: 78rpx;
  background: #D5D5D5;
  border-radius: 100rpx;   
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #FFFFFF;
  text-align: center;

}
.open_setting_container .cancel::after{
  border : none;
}
.open_setting_container .confirm{
  display: inline-block;
  overflow: inherit;
  padding: 0;
  font-size: 30rpx;
  text-align: center;
  margin : 0 30rpx;
  width : 264rpx;
  height : 78rpx;
  line-height: 78rpx;
  background-color: #26BCC5;
  border-radius: 100rpx;
  color: #FFFFFF;
}

.open_setting_container .confirm::after{
  border : none;
}