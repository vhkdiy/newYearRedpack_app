.container {
  position: relative;
  width: 100%;
  height: 100%;
  padding: 12rpx;
  box-sizing: border-box;
}

.chose_img_bg{
  width: 100%;
  height: 100%;
}

.top_img{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
}

.touchingTopImg{
  opacity: 0.6;
}

.img {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  overflow: hidden;
}

.img image {
  height: 400px;
}

.imgcrop {
  position: absolute;
  left: -50000rpx;
  top: -500000rpx;
}

.painter{
  position: absolute;
  left: -1000rpx;
  top: -5000rpx;
  z-index: 1000;
}