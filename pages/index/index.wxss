.problem_text {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #3a3a70;
  font-weight: bold;
  -webkit-box-flex: 1;
  margin-left: 18rpx;
  padding-top: 6rpx;
}

.problem_img {
  width: 480rpx;
  height: 240rpx;
  display: block;
  margin: 18rpx auto 0;
}

/*  */

.radio_item {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #3a3a70;
  width: 484rpx;
  font-weight: bold;
  margin: 40rpx auto 0;
  padding-left: 56rpx;
  position: relative;
  height: 78rpx;
  line-height: 72rpx;
  box-sizing: border-box;
}

.radio_item_content {
  position: absolute;
  z-index: 2;
  width: 100%;
  left: 0;
  top: 0;
  background: #fff;
  border: 4rpx solid #3a3a70;
  border-radius: 6rpx;
  padding-left: 56rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 100%;
}

.radio_item_ditu {
  position: absolute;
  left: 2rpx;
  right: 2rpx;
  height: 100%;
  background: #3a3a70;
  border-radius: 6rpx;
  box-sizing: border-box;
  top: 8rpx;
  content: '';
  z-index: 0;
}

.radio_item_point {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 16rpx;
  height: 28rpx;
  width: 28rpx;
  background: rgba(193, 193, 255, 0.3);
  border-radius: 100%;
  z-index: 2;
}

.selected .radio_item_point {
  background: #fff;
}

.selected .radio_item_content {
  background: #ffee57;
}

.selected .radio_item_ditu {
  left: 0;
  right: 0;
}

/* 选择框 */

.h_line {
  background: #d8d8d8;
  height: 2rpx;
  margin-top: 42rpx;
}

.date_section {
  background: #fff;
  border: 4rpx solid #3a3a70;
  border-radius: 6rpx;
  width: 484rpx;
  height: 78rpx;
  line-height: 74rpx;
  margin: 30rpx auto 0;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #999;
  position: relative;
  margin-bottom: 36rpx;
  font-weight: bold;
  box-sizing: border-box;
}

.date_section_selected {
  color: #3a3a70;
}

.picker {
  padding-left: 18rpx;
}

.fade_btn {
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  opacity: 0;
}

.input_section {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #3a3a70;
  display: block;
  margin: 30rpx auto 0;
  margin-bottom: 36rpx;
  font-weight: bold;
  border: 4rpx solid #3a3a70;
  border-radius: 6rpx;
  width: 484rpx;
  height: 78rpx;
  line-height: 78rpx;
  padding: 0 18rpx;
  box-sizing: border-box;
}

.input_section::-webkit-input-placeholder {
  color: #999;
}

.commin_btn_gray {
  width: 484rpx;
  border-radius: 6rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  background: #ccc;
  text-align: center;
  font-weight: bold;
  color: #fff;
  box-sizing: border-box;
}

.bottom_commit_btn {
  margin: 75rpx auto 28rpx;
  position: relative;
}

.inner_btn_gray {
  height: 78rpx;
  line-height: 78rpx;
}

.inner_btn_gray_bottom {
  height: 13rpx;
  border-radius: 0 0 6rpx 6rpx;
  background-color: #fff;
  box-sizing: border-box;
  border: 4rpx solid #ccc;
  border-top: 0;
}

.commit_btn_blue {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #3a3a70;
  width: 484rpx;
  text-align: center;
  font-weight: bold;
}

.inner_btn_blue {
  background: #ffee57;
  border: 4rpx solid #3a3a70;
  box-sizing: border-box;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 6rpx;
}

.inner_btn_blue_bottom {
  background: #3a3a70;
  border-radius: 0 0 6rpx 6rpx;
  height: 12rpx;
  width: 480rpx;
  margin: 0 auto;
  margin-top: -2rpx;
  box-sizing: border-box;
}

.img_group{
  margin-top: 25rpx;
  margin-left: 82rpx;
}

.img_radio_item {
  width: 242rpx;
  height: 198rpx;
  position: relative;
  overflow: hidden;
  display: inline-block;
  background: #ffee57;
  border: 4rpx solid #3a3a70;
  box-sizing: border-box;
  border-radius: 12rpx;
