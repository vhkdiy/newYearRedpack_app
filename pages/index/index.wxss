page {
  background: #fbcc0e;
  padding-bottom: 54rpx;
}

.no_start_title {
  display: block;
  width: 100%;
  height: 286rpx;
}

/* 未开始的说明 */

.play_tip_container {
  background: #fff;
  border: 3rpx solid #3a3a70;
  border-radius: 20rpx;
  display: -webkit-box;
  -webkit-box-align: center;
  padding-right: 340rpx;
  box-sizing: border-box;
  width: 642rpx;
  min-height: 277rpx;
  position: relative;
  margin: 33rpx auto 0;
  padding-left: 42rpx;
}

.play_tip_container::before {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #3a3a70;
  border: 3rpx solid #3a3a70;
  border-radius: 20rpx;
  top: 16rpx;
  left: 16rpx;
  content: '';
  z-index: -1;
}

.play_tip_text {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #2b2b52;
  -webkit-box-flex: 1;
}

.play_tip_img {
  height: 300rpx;
  width: 330rpx;
  position: absolute;
  right: 1prx;
  bottom: 0;
  z-index: 1;
}

.zhuanshi_left {
  position: absolute;
  width: 25rpx;
  height: 10rpx;
  left: -14rpx;
  top: 203rpx;
}

.zhuanshi_right {
  position: absolute;
  width: 34rpx;
  height: 10rpx;
  right: -24rpx;
  top: 67rpx;
}

.start_btn {
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #3a3a70;
  width: 652rpx;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  background: #ffee57;
  border: 4rpx solid #3a3a70;
  border-radius: 10rpx;
  margin: 76rpx auto 0;
  position: relative;
  box-sizing: border-box;
  font-weight: bold;
}

.start_btn::before {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #3a3a70;
  border-radius: 10rpx;
  top: 14rpx;
  left: 0;
  content: '';
  z-index: -1;
}

.ad_img {
  display: block;
  width: 666rpx;
  height: 224rpx;
  margin: 91rpx auto 0;
  border: 1px solid #fff;
  border-radius: 20rpx;
}

/* 开始答题部分 */

.start_title {
  height: 60rpx;
  width: 420rpx;
  display: block;
  margin: 24rpx auto 0;
}

.answer_container {
  background: #fff;
  border: 3rpx solid #3a3a70;
  border-radius: 20rpx;
  box-sizing: border-box;
  width: 642rpx;
  min-height: 277rpx;
  position: relative;
  margin: 33rpx auto 0;
  margin-top: 91rpx;
  padding-bottom: 46rpx;
}

.answer_container:first-of-type {
  margin-top: 54rpx;
}

.answer_container::before {
  position: absolute;
  width: 100%;
  height: 100%;
  background: #3a3a70;
  border: 3rpx solid #3a3a70;
  border-radius: 20rpx;
  top: 16rpx;
  left: 16rpx;
  content: '';
  z-index: -1;
}

.answer_container .zhuanshi_left {
  top: 283rpx;
}

.answer_container .zhuanshi_right {
  top: 118rpx;
}

.problem_container {
  display: -webkit-box;
  -webkit-box-align: center;
  margin-top: 42rpx;
  margin-left: -30rpx;
}

.page_number_container {
  background: #ffee57;
  border: 4rpx solid #3a3a70;
  border-radius: 100px;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #3a3a70;
  font-weight: bold;
  min-width: 134rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
}

.problem_icon_container {
  width: 112rpx;
  height: 98rpx;
  border-radius: 100px;
  position: relative;
}

.problem_icon {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}

